{
    "name": "hyperf-ext/hashing",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "hashing"
    ],
    "description": "The Hyperf 3 Hashing package.\n fork from hyperf-ext/hashing",
    "authors": [
        {
            "name": "tokio",
            "email": "ellenzhao888888@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HyperfExt\\Hashing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "hyperf/config": "~3.1.0",
        "hyperf/di": "^3.1",
        "hyperf/framework": "~3.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/testing": "~3.1.0",
        "phpstan/phpstan": "^1.0",
        "swoole/ide-helper": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --prepend tests/bootstrap.php -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "HyperfExt\\Hashing\\ConfigProvider"
        }
    }
}
